{
    "name": "tiltbuilt/boilermaker-tr",
    "description": "Starter Craft setup for site built by Tilt.",
    "version": "2.7.0",
    "type": "project",
    "require": {        
        "craftcms/ckeditor": "3.6.0",
        "craftcms/cms": "4.5.9",
        "craftcms/feed-me": "5.2.0",        
        "doublesecretagency/craft-cpcss": "2.6.0",
        "mmikkel/retcon": "2.7.4",
        "nystudio107/craft-minify": "4.0.0-beta.2",
        "nystudio107/craft-retour": "4.1.14",
        "nystudio107/craft-seomatic": "4.0.33",
        "nystudio107/craft-twigpack": "4.0.0-beta.4",
        "percipioglobal/craft-colour-swatches": "4.3.0",
        "putyourlightson/craft-blitz": "4.7.0",
        "putyourlightson/craft-elements-panel": "2.0.0",
        "sebastianlenz/linkfield": "2.1.5",
        "solspace/craft-freeform": "4.1.11",
        "spacecatninja/imager-x": "4.2.3",
        "spicyweb/craft-embedded-assets": "3.1.8",
        "spicyweb/craft-neo": "3.9.8",        
        "verbb/field-manager": "3.0.8",
        "verbb/icon-picker": "2.0.15",
        "verbb/navigation": "2.0.22",
        "vlucas/phpdotenv": "^5.4.0",
        "wbrowar/guide": "3.2.1",
        "wmd/section-and-product-type": "2.0.3"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.craftcms.com",
            "canonical": false
        }
    ]
}