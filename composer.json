{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/ckeditor": "^4.9.0",
    "craftcms/cms": "^5.8.12",
    "craftcms/feed-me": "^6.8.0",
    "craftcms/postmark": "^3.1.0",
    "craftpulse/craft-colour-swatches": "^5.1.0",
    "mmikkel/retcon": "^3.2.1",
    "nystudio107/craft-minify": "^5.0.0",
    "nystudio107/craft-retour": "^5.0.11",
    "nystudio107/craft-seomatic": "^5.1.16",
    "nystudio107/craft-typogrify": "^5.0.1",
    "nystudio107/craft-vite": "^5.0.1",
    "putyourlightson/craft-blitz": "^5.12.0",
    "putyourlightson/craft-datastar": "1.0.0-RC.4",
    "servd/craft-asset-storage": "^4.1.2",
    "spacecatninja/imager-x": "^5.1.6",
    "spacecatninja/imager-x-power-pack": "^1.0.5",
    "spicyweb/craft-embedded-assets": "^5.4.3",
    "verbb/buttonbox": "^5.0.0",
    "verbb/cloner": "^3.0.2",
    "verbb/color-mixer": "^4.0.1",
    "verbb/default-dashboard": "^3.0.2",
    "verbb/element-index-defaults": "^4.0.1",
    "verbb/expanded-singles": "^3.0.3",
    "verbb/field-manager": "^4.0.3",
    "verbb/formie": "^3.1.1",
    "verbb/icon-picker": "^3.0.5",
    "verbb/knock-knock": "^3.0.4",
    "verbb/navigation": "^3.0.10",
    "verbb/shortcodes": "^4.0.1",
    "verbb/social-share": "^2.0.5",
    "verbb/tablemaker": "^5.0.6",
    "vlucas/phpdotenv": "^5.4.0",
    "wmd/section-and-product-type": "^2.0.8"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "treeware/plant": false,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "craft-update": [
      "@pre-craft-update",
      "@post-craft-update"
    ],
    "pre-craft-update": [],
    "post-craft-update": [
      "@php craft install/check && php craft up --interactive=0 || exit 0",
      "@php craft install/check && php craft clear-caches/all --interactive=0 || exit 0",
      "@php craft install/check && php craft invalidate-tags/all --interactive=0 || return 0"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php craft setup/welcome"
    ],
    "pre-update-cmd": "@pre-craft-update",
    "pre-install-cmd": "@pre-craft-update",
    "post-update-cmd": "@post-craft-update",
    "post-install-cmd": "@post-craft-update"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}