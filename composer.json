{
  "name": "tiltbuilt/boilermaker-tr",
  "description": "Starter Craft setup for site built by Tilt.",
  "version": "1.0.0",
  "type": "project",
  "require": {
    "carlcs/craft-redactorcustomstyles": "3.0.4",
    "craftcms/cms": "3.6.6",
    "craftcms/feed-me": "4.3.5.1",
    "craftcms/redactor": "2.8.5",
    "fruitstudios/linkit": "1.1.12.1",
    "mmikkel/retcon": "2.2.1",
    "nystudio107/craft-imageoptimize": "1.6.23",
    "nystudio107/craft-retour": "3.1.46",
    "nystudio107/craft-seomatic": "3.3.32",
    "nystudio107/craft-twigpack": "1.2.9",
    "pennebaker/craft-architect": "2.4.1",
    "putyourlightson/craft-blitz": "3.7.0",
    "putyourlightson/craft-blitz-recommendations": "1.2.0",
    "solspace/craft-freeform": "3.10.8",
    "spacecatninja/imager-x": "v3.3.0",
    "spicyweb/craft-neo": "2.8.19",
    "topshelfcraft/environment-label": "3.2.0",
    "utakka/redactor-anchors": "1.1.0",
    "verbb/field-manager": "2.2.2",
    "verbb/icon-picker": "1.1.10",
    "verbb/navigation": "1.4.13",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
