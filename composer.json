{
    "name": "tiltbuilt/boilermaker-tr",
    "description": "Starter Craft setup for site built by Tilt.",
    "version": "2.1.0",
    "type": "project",
    "require": {
        "carlcs/craft-redactorcustomstyles": "4.0.2",
        "craftcms/cms": "4.1.1",
        "craftcms/feed-me": "5.0.4",
        "craftcms/redactor": "3.0.2",
        "doublesecretagency/craft-cpcss": "2.5.0",
        "mmikkel/retcon": "2.4.2",
        "nystudio107/craft-minify": "4.0.0-beta.2",
        "nystudio107/craft-retour": "4.0.2",
        "nystudio107/craft-seomatic": "4.0.6",
        "nystudio107/craft-twigpack": "4.0.0-beta.4",
        "percipioglobal/craft-colour-swatches": "4.1.0",
        "putyourlightson/craft-blitz": "4.2.0",
        "putyourlightson/craft-elements-panel": "2.0.0",
        "sebastianlenz/linkfield": "2.1.3-rc",
        "solspace/craft-freeform": "4.0.0-beta.13",
        "spacecatninja/imager-x": "4.1.1",
        "spicyweb/craft-neo": "3.1.5",
        "utakka/redactor-anchors": "1.4.0",
        "verbb/field-manager": "3.0.0-beta.1",
        "verbb/icon-picker": "dev-craft-4 as 2.0.0-beta.5",
        "verbb/navigation": "2.0.3",
        "vlucas/phpdotenv": "^5.4.0",
        "wbrowar/guide": "3.2.0",
        "wmd/section-and-product-type": "1.1.0"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
